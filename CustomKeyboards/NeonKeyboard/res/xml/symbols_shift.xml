<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyHeight="@dimen/key_height"
    android:keyWidth="10%p"
    android:verticalGap="0px" >

    <Row>
        <Key
            android:codes="126"
            android:keyEdgeFlags="left"
            android:keyLabel="~" />
        <Key
            android:codes="177"
            android:keyLabel="±" />
        <Key
            android:codes="215"
            android:keyLabel="×" />
        <Key
            android:codes="247"
            android:keyLabel="÷" />
        <Key
            android:codes="8226"
            android:keyLabel="•" />
        <Key
            android:codes="176"
            android:keyLabel="°" />
        <Key
            android:codes="96"
            android:keyLabel="`" />
        <Key
            android:codes="180"
            android:keyLabel="´" />
        <Key
            android:codes="123"
            android:keyLabel="{" />
        <Key
            android:codes="125"
            android:keyEdgeFlags="right"
            android:keyLabel="}" />
    </Row>
    <Row>
        <Key
            android:codes="169"
            android:keyEdgeFlags="left"
            android:keyLabel="©" />
        <Key
            android:codes="163"
            android:keyLabel="£" />
        <Key
            android:codes="8364"
            android:keyLabel="€" />
        <Key
            android:codes="94"
            android:keyLabel="^" />
        <Key
            android:codes="174"
            android:keyLabel="®" />
        <Key
            android:codes="165"
            android:keyLabel="¥" />
        <Key
            android:codes="95"
            android:keyLabel="_" />
        <Key
            android:codes="43"
            android:keyLabel="+" />
        <Key
            android:codes="91"
            android:keyLabel="[" />
        <Key
            android:codes="93"
            android:keyEdgeFlags="right"
            android:keyLabel="]" />
    </Row>
    <Row>
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/shift_icon"
            android:keyWidth="15%p" />
        <Key
            android:codes="161"
            android:keyLabel="¡" />
        <Key
            android:codes="60"
            android:keyLabel="&lt;" />
        <Key
            android:codes="62"
            android:keyLabel=">" />
        <Key
            android:codes="162"
            android:keyLabel="¢" />
        <Key
            android:codes="124"
            android:keyLabel="|" />
        <Key
            android:codes="92"
            android:keyLabel="\\" />
        <Key
            android:codes="191"
            android:keyLabel="¿" />
        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/delete_icon"
            android:keyWidth="15%p" />
    </Row>
    <Row  >
        <Key
            android:codes="-2"
            android:keyEdgeFlags="left"
            android:keyLabel="ABC"
            android:keyWidth="15%p" />
        <Key
            android:codes="-8"
           
            android:keyIcon="@drawable/settings_icon"
            android:keyWidth="15%p" />
        <Key
            android:codes="32"
            android:isRepeatable="true"
            android:keyIcon="@drawable/space_icon"
            android:keyWidth="35%p" />
        <Key
            android:codes="8230"
            android:keyLabel="…"
            android:keyWidth="15%p" />
        <Key
            android:codes="10"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_return"
            android:keyWidth="20%p" />
    </Row>
    <Row android:rowEdgeFlags="bottom" >
        <Key
            android:codes="19"
            android:isRepeatable="true"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/icon_arrow_up"
            android:keyWidth="25%p" />
        <Key
            android:codes="20"
            android:isRepeatable="true"
            android:keyIcon="@drawable/icon_arrow_down"
            android:keyWidth="25%p" />
        <Key
            android:codes="21"
            android:isRepeatable="true"
            android:keyIcon="@drawable/icon_arrow_left"
            android:keyWidth="25%p" />
        <Key
            android:codes="22"
            android:isRepeatable="true"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/icon_arrow_right"
            android:keyWidth="25%p" />
    </Row>

</Keyboard>